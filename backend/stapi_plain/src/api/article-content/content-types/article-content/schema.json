{
  "kind": "collectionType",
  "collectionName": "article_contents",
  "info": {
    "singularName": "article-content",
    "pluralName": "article-contents",
    "displayName": "article_content",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "article_contents"
    },
    "showcase": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "recommended": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "content_details": {
      "type": "richtext"
    },
    "video_link": {
      "type": "text"
    },
    "left_article": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "right_article": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "top_section": {
      "type": "boolean",
      "default": false
    },
    "top_article": {
      "type": "boolean",
      "default": false
    },
    "also_melt": {
      "type": "boolean",
      "default": false
    },
    "author": {
      "type": "string"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "article_contents"
    }
  }
}
